{
  "name": "bug-fluentui-menu",
  "productName": "bug-fluentui-menu",
  "version": "1.0.0",
  "description": "electron gui for redis",
  "main": ".webpack/main",
  "scripts": {
    "dev": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "Chris Zheng",
    "email": "z@caudate.me"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "kmi_redis_gui"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./config/webpack.config.frame.js",
            "renderer": {
              "config": "./config/webpack.config.inner.js",
              "entryPoints": [
                {
                  "html": "./main-inner.html",
                  "js": "./main-inner.jsx",
                  "name": "main_inner"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@sucrase/webpack-loader": "^2.0.0",
    "babel-loader": "^8.2.2",
    "cache-loader": "4.1.0",
    "css-loader": "^4.2.1",
    "electron": "11.2.0",
    "electron-context-menu": "^2.4.0",
    "electron-react-devtools": "^0.5.3",
    "electron-squirrel-startup": "1.0.0",
    "node-loader": "^1.0.1",
    "raw-loader": "^4.0.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^1.2.1",
    "sucrase": "^3.17.0"
  },
  "dependencies": {
    "@fluentui/react-northstar": "0.51.5",
    "immer": "^8.0.1",
    "react": "16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "5.2.0",
    "use-immer": "^0.4.2"
  }
}
